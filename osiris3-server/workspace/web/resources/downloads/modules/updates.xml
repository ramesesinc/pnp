<app>
    <env>
		app.title=ePIMS (Sariaya)
        app.host=localhost:8070
		app.cluster=osiris3
        app.context=epims
		app.help=http://localhost:8080/help
		app.loader=com.rameses.osiris2.client.OsirisAppLoader
		app.debug=true
		readTimeout=50000
		connectionTimeout=5000
    </env>    
    <modules>
		<!--  osiris3 core libraries -->
		<module file="core/osiris2-client-themes.jar" 	version="1.03"/>
		<module file="core/osiris2-client-ui.jar" 		version="1.03"/>
		<module file="core/osiris2-framework.jar" 		version="1.03"/>
		<module file="core/osiris2-report.jar" 			version="1.03"/>
		<module file="core/rameses-client-ui.jar" 		version="1.03"/>
		<module file="core/rameses-client-ui-bio.jar" 	version="1.03"/>
		<module file="core/rameses-common2.jar" 		version="1.03"/>
		<module file="core/rameses-custom-impl.jar" 	version="1.03"/>

		<!-- client shared modules -->
		<module file="shared/client-system.jar" 			version="1.03"/>
		<module file="shared/client-ui-extended.jar" 		version="1.03"/>
		<module file="shared/rameses-client-menu.jar" 		version="1.03"/>
		<module file="shared/rameses-client-report.jar" 	version="1.03"/>
		<module file="shared/rameses-enterprise-admin.jar"  version="1.03"/>
		<module file="shared/rameses-enterprise-entity.jar" version="1.03"/>

		<!-- plugin modules --> 
		<module file="rameses-pnp-main.jar" 				version="2.01"/> 
		<module file="rameses-gov-police.jar" 				version="2.01"/>  
		<module file="rameses-gov-police-report.jar" 		version="2.01"/> 
		<module file="rameses-gov-signatory-template.jar" 	version="2.01"/> 
    </modules>
</app>
