<app>
    <env>
		app.title=ePIMS (Sariaya)
        app.host=localhost:8070
		app.cluster=osiris3
        app.context=epims
		app.help=http://localhost:8080/help
		app.loader=com.rameses.osiris2.client.OsirisAppLoader
		app.debug=true
		readTimeout=50000
		connectionTimeout=5000
    </env>    
    <modules>
		<!--  osiris3 core libraries -->
		<module file="core/osiris2-client-themes.jar" />
		<module file="core/osiris2-client-ui.jar" />
		<module file="core/osiris2-framework.jar" />
		<module file="core/osiris2-report.jar" />
		<module file="core/rameses-client-ui.jar" />
		<module file="core/rameses-client-ui-bio.jar" />
		<module file="core/rameses-common2.jar" />
		<module file="core/rameses-custom-impl.jar" />

		<!-- client shared modules -->
		<module file="shared/client-system.jar" />
		<module file="shared/client-ui-extended.jar" />
		<module file="shared/rameses-client-menu.jar" />
		<module file="shared/rameses-client-report.jar" />
		<module file="shared/rameses-enterprise-admin.jar" />
		<module file="shared/rameses-enterprise-entity.jar"  />

		<!-- plugin modules --> 
		<module file="rameses-gov-police.jar" />
		<module file="rameses-pnp-main.jar" />
    </modules>
</app>
